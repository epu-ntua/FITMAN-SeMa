<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		elementFormDefault="qualified">

	<xsd:include schemaLocation="countries.xsd"/>
	<xsd:include schemaLocation="currencies.xsd"/>
	<xsd:include schemaLocation="lang.xsd"/>
	<xsd:include schemaLocation="punit.xsd"/>
	<xsd:include schemaLocation="unit.xsd"/>

	<xsd:simpleType name="dtBOOLEAN">
		<xsd:restriction base="dtSTRING">
			<xsd:pattern value="[Ff][Aa][Ll][Ss][Ee]|[Tt][Rr][Uu][Ee]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="dt_COUNT">
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- The xsd:dateTime-datatype of W3C-XSD is a small part of the Datetime-format 
		of ISO 8601 standard, therefore the ISO 8601-Format is modelled separately, 
		Format: CCYY-MM-DDThh:mm:ss.sTZD, see datatype "dtDATETIME"
	-->
	<xsd:simpleType name="dtDATETIME">
		<xsd:restriction base="xsd:string">
 			<xsd:pattern value="\d{4}(-(0[1-9]|1[0-2])(-(0[1-9]|1[0-9]|2[0-9]|3[0-1])(T(0[0-9]|1[0-9]|2[0-3])(:[0-5][0-9])(:[0-5][0-9](\.[0-9]{1,}){0,1}){0,1}(([+-]([0-1][0-9]|2[0-3])(:[0-5][0-9]))|Z){0,1}){0,1}){0,1}){0,1}"/> 
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="dtFLOAT">
		<xsd:restriction base="xsd:float"/>
	</xsd:simpleType>
	<xsd:simpleType name="dtINTEGER">
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="dtNUMBER">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="dtSTRING">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="typeBUYER_AID">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeCOST_CATEGORY_ID">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="64"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typePAYMENT_TERM">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="250"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeINTERNATIONAL_AID">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="32"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeINTERNATIONAL_RESTRICTIONS">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="250"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typePARTY_ID">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="250"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typePHONE">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typePUBLIC_KEY">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="64000"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeREMARK">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="64000"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeSPECIAL_TREATMENT_CLASS">
		<xsd:restriction base="dtSTRING">
			<xsd:maxLength value="20"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="ACCOUNT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="HOLDER"/>
				<xsd:element ref="BANK_NAME"/>
				<xsd:element ref="BANK_COUNTRY"/>
				<xsd:element ref="BANK_CODE"/>
				<xsd:element ref="BANK_ACCOUNT"/>
				<xsd:element ref="PAYMENT_TERM" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ACCOUNTING_INFO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="COST_CATEGORY_ID"/>
				<xsd:element ref="COST_TYPE" minOccurs="0"/>
				<xsd:element ref="COST_ACCOUNT" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ADDRESS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="NAME"/>
				<xsd:element ref="NAME2" minOccurs="0"/>
				<xsd:element ref="NAME3" minOccurs="0"/>
				<xsd:element ref="DEPARTMENT" minOccurs="0"/>
				<xsd:element ref="CONTACT" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="STREET" minOccurs="0"/>
				<xsd:element ref="ZIP" minOccurs="0"/>
				<xsd:element ref="BOXNO" minOccurs="0"/>
				<xsd:element ref="ZIPBOX" minOccurs="0"/>
				<xsd:element ref="CITY" minOccurs="0"/>
				<xsd:element ref="STATE" minOccurs="0"/>
				<xsd:element ref="COUNTRY" minOccurs="0"/>
				<xsd:element ref="VAT_ID" minOccurs="0"/>
				<xsd:element ref="PHONE" minOccurs="0"/>
				<xsd:element ref="FAX" minOccurs="0"/>
				<xsd:element ref="EMAIL" minOccurs="0"/>
				<xsd:element ref="PUBLIC_KEY" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="URL" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
		<!-- ***** START deactivate identity-constraints ***** -->
		<xsd:unique name="uniquePUBLIC_KEYtype">
			<xsd:selector xpath="PUBLIC_KEY"/>
			<xsd:field xpath="@type"/>
		</xsd:unique>
		<xsd:unique name="uniqueCONTACT_NAME">
			<xsd:selector xpath="CONTACT"/>
			<xsd:field xpath="CONTACT_NAME"/>
		</xsd:unique>
		<!-- ***** END deactivate identity-constraints   ***** -->
	</xsd:element>
	<xsd:element name="AGREEMENT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AGREEMENT_ID"/>
				<xsd:element ref="AGREEMENT_LINE_ID" minOccurs="0"/>
				<xsd:element ref="AGREEMENT_START_DATE" minOccurs="0"/>
				<xsd:element ref="AGREEMENT_END_DATE" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="type" use="required">
				<xsd:simpleType>
					<xsd:restriction base="dtSTRING">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="50"/>
						<!-- ***** predefined values for attribute 'type' of element 'AGREEMENT' *****
							"(buyer | supplier)"
						-->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AGREEMENT_END_DATE" type="dtDATETIME"/>
	<xsd:element name="AGREEMENT_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AGREEMENT_LINE_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="32"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AGREEMENT_START_DATE" type="dtDATETIME"/>
	<xsd:element name="ALT_CUSTOMER_ORDER_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ARTICLE_ID">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SUPPLIER_AID"/>
				<xsd:element ref="INTERNATIONAL_AID" minOccurs="0"/>
				<xsd:element ref="BUYER_AID" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="DESCRIPTION_SHORT" minOccurs="0"/>
				<xsd:element ref="DESCRIPTION_LONG" minOccurs="0"/>
				<xsd:element ref="MANUFACTURER_INFO" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
		<!-- ***** START deactivate identity-constraints ***** -->
		<xsd:unique name="uniqueBUYER_AID">
			<xsd:selector xpath="BUYER_AID"/>
			<xsd:field xpath="@type"/>
		</xsd:unique>
		<!-- ***** END deactivate identity-constraints   ***** -->
	</xsd:element>
	<xsd:element name="ARTICLE_PRICE">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PRICE_AMOUNT"/>
				<xsd:element ref="PRICE_FLAG" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="PRICE_LINE_AMOUNT"/>
				<xsd:element ref="TAX" minOccurs="0"/>
				<xsd:element ref="TAX_AMOUNT" minOccurs="0"/>
				<xsd:element ref="PRICE_QUANTITY" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="type" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:NMTOKEN">
						<xsd:pattern value="net_list|gros_list|net_customer|nrp|net_customer_exp|udp_\w{1,16}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
		<!-- ***** START deactivate identity-constraints ***** -->
		<xsd:unique name="uniquePRICE_FLAG">
			<xsd:selector xpath="PRICE_FLAG"/>
			<xsd:field xpath="@type"/>
		</xsd:unique>
		<!-- ***** END deactivate identity-constraints   ***** -->
	</xsd:element>
	<xsd:element name="BANK_ACCOUNT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="BANK_CODE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="20"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="BANK_COUNTRY" type="dtCOUNTRIES"/>
	<xsd:element name="BANK_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="BOXNO">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="20"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="BUYER_AID">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typeBUYER_AID">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:maxLength value="50"/>
								<xsd:minLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="BUYER_PARTY">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PARTY"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CARD">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CARD_NUM"/>
				<xsd:element ref="CARD_AUTH_CODE" minOccurs="0"/>
				<xsd:element ref="CARD_REF_NUM" minOccurs="0"/>
				<xsd:element ref="CARD_EXPIRATION_DATE"/>
				<xsd:element ref="CARD_TYPE"/>
				<xsd:element ref="CARD_TYPE_OTHER" minOccurs="0"/>
				<xsd:element ref="CARD_HOLDER_NAME"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CARD_AUTH_CODE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="10"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CARD_EXPIRATION_DATE" type="dtDATETIME"/>
	<xsd:element name="CARD_HOLDER_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CARD_NUM">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="16"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CARD_REF_NUM">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CARD_TYPE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CARD_TYPE_OTHER">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CASH">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PAYMENT_TERM"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CATALOG_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="20"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CATALOG_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="100"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CATALOG_REFERENCE">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CATALOG_ID"/>
				<xsd:element ref="CATALOG_VERSION"/>
				<xsd:element ref="CATALOG_NAME" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CATALOG_VERSION">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="7"/>
				<xsd:minLength value="3"/>
				<xsd:pattern value="[0-9]{1,3}\.[0-9]{1,3}"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CHECK">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PAYMENT_TERM"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CITY">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CONTACT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CONTACT_NAME"/>
				<xsd:element ref="PHONE" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="FAX" minOccurs="0"/>
				<xsd:element ref="EMAIL" minOccurs="0"/>
				<xsd:element ref="PUBLIC_KEY" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="URL" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
		<!-- ***** START deactivate identity-constraints ***** -->
		<xsd:unique name="uniquePUBLIC_KEYtype2">
			<xsd:selector xpath="PUBLIC_KEY"/>
			<xsd:field xpath="@type"/>
		</xsd:unique>
		<!-- ***** END deactivate identity-constraints   ***** -->
	</xsd:element>
	<xsd:element name="CONTACT_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CONTROL_INFO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="STOP_AUTOMATIC_PROCESSING" minOccurs="0"/>
				<xsd:element ref="GENERATOR_INFO" minOccurs="0"/>
				<xsd:element ref="GENERATION_DATE" minOccurs="0"/>
				<xsd:element ref="MIME_ROOT" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="COST_ACCOUNT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="64"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="COST_CATEGORY_ID">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typeCOST_CATEGORY_ID">
					<xsd:attribute name="type" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:NMTOKEN">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="32"/>
								<xsd:enumeration value="cost_center"/>
								<xsd:enumeration value="project"/>
								<xsd:enumeration value="work_order"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="COST_TYPE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="64"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="COUNTRY" type="dtCOUNTRIES"/>
	<xsd:element name="DEBIT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PAYMENT_TERM"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DELIVERY_COMPLETED" type="dtBOOLEAN"/>
	<xsd:element name="DELIVERY_DATE">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DELIVERY_START_DATE"/>
				<xsd:element ref="DELIVERY_END_DATE"/>
			</xsd:sequence>
			<xsd:attribute name="type" default="fixed">
				<xsd:simpleType>
					<xsd:restriction base="xsd:NMTOKEN">
						<xsd:enumeration value="optional"/>
						<xsd:enumeration value="fixed"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DELIVERY_END_DATE" type="dtDATETIME"/>
	<xsd:element name="DELIVERYNOTE_DATE" type="dtDATETIME"/>
	<xsd:element name="DELIVERYNOTE_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DELIVERY_PARTY">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PARTY"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DELIVERY_REFERENCE">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DELIVERYNOTE_ID"/>
				<xsd:element ref="LINE_ITEM_ID"/>
				<xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DELIVERY_START_DATE" type="dtDATETIME"/>
	<xsd:element name="DEPARTMENT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DESCRIPTION_LONG">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="64000"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DESCRIPTION_SHORT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="80"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DISPATCHNOTIFICATION_DATE" type="dtDATETIME"/>
	<xsd:element name="DISPATCHNOTIFICATION_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="EMAIL">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="100"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="FAX">
		<xsd:simpleType>
