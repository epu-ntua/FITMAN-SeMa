						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="50"/>
								<!-- ***** predefined values for attribute 'type' of element 'INTERNATIONAL_AID' *****
									"(EAN | GTIN)" 
								-->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="INTERNATIONAL_RESTRICTIONS">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typeINTERNATIONAL_RESTRICTIONS">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="32"/>
								<!-- ***** predefined values for attribute 'type' of element 'INTERNATIONAL_RESTRICTIONS' *****
									"(national | us-embargo | un-embargo | wto-embargo | other)" 
								-->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="INVOICE_DATE" type="dtDATETIME"/>
	<xsd:element name="INVOICE_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="250"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="INVOICE_PARTY">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PARTY"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="LINE_ITEM_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="LOCATION">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MANUFACTURER_AID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MANUFACTURER_INFO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MANUFACTURER_NAME"/>
				<xsd:element ref="MANUFACTURER_AID"/>
				<xsd:element ref="MANUFACTURER_TYPE_DESCR" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MANUFACTURER_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MANUFACTURER_TYPE_DESCR">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MEANS_OF_TRANSPORT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MEANS_OF_TRANSPORT_ID"/>
				<xsd:element ref="MEANS_OF_TRANSPORT_NAME" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="type">
				<xsd:simpleType>
					<xsd:restriction base="dtSTRING">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="50"/>
						<!-- ***** predefined values for attribute 'type' of element 'MEANS_OF_TRANSPORT' *****
							"(maritime | rail | road | air | multimodal)"
						-->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MEANS_OF_TRANSPORT_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="32"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MEANS_OF_TRANSPORT_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MIME_TYPE" minOccurs="0"/>
				<xsd:element ref="MIME_SOURCE"/>
				<xsd:element ref="MIME_DESCR" minOccurs="0"/>
				<xsd:element ref="MIME_ALT" minOccurs="0"/>
				<xsd:element ref="MIME_PURPOSE" minOccurs="0"/>
				<xsd:element ref="MIME_ORDER" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MIME_ALT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_DESCR">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_INFO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MIME" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MIME_ORDER" type="dtINTEGER"/>
	<xsd:element name="MIME_PURPOSE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="20"/>
				<!-- ***** predefined values for element 'MIME_PURPOSE' *****
					"(thumbnail | normal | detail | data_sheet | logo | others)" 
				-->
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_ROOT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="100"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_SOURCE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_TYPE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="30"/>
				<xsd:minLength value="1"/>
				<!-- ***** predefined values for element 'MIME_TYPE' *****
					"(url | application/pdf | image/jpeg | image/gif | text/html | text/plain | ...)" 
				-->
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NAME2">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NAME3">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ORDERCHANGE_DATE" type="dtDATETIME"/>
	<xsd:element name="ORDERCHANGE_SEQUENCE_ID" type="dt_COUNT"/>
	<xsd:element name="ORDERRESPONSE_DATE" type="dtDATETIME"/>
	<xsd:element name="ORDER_DATE" type="dtDATETIME"/>
	<xsd:element name="ORDER_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ORDER_ITEM">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="LINE_ITEM_ID"/>
				<xsd:element ref="ARTICLE_ID"/>
				<xsd:element ref="QUANTITY"/>
				<xsd:element ref="ORDER_UNIT"/>
				<xsd:element ref="ARTICLE_PRICE" minOccurs="0"/>
				<xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
				<xsd:element ref="SOURCING_INFO" minOccurs="0"/>
				<xsd:element ref="SHIPMENT_PARTIES" minOccurs="0"/>
				<xsd:element ref="ACCOUNTING_INFO" minOccurs="0"/>
				<xsd:element ref="PARTIAL_SHIPMENT_ALLOWED" minOccurs="0"/>
				<xsd:element ref="TRANSPORT" minOccurs="0"/>
				<xsd:element ref="INTERNATIONAL_RESTRICTIONS" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="SPECIAL_TREATMENT_CLASS" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="MIME_INFO" minOccurs="0"/>
				<xsd:element ref="REMARK" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
		<!-- ***** START deactivate identity-constraints ***** -->
		<xsd:unique name="uniqueREMARK">
			<xsd:selector xpath="REMARK"/>
			<xsd:field xpath="@type"/>
		</xsd:unique>
		<!-- ***** END deactivate identity-constraints   ***** -->
	</xsd:element>
	<xsd:element name="ORDER_PARTIES">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="BUYER_PARTY"/>
				<xsd:element ref="SUPPLIER_PARTY"/>
				<xsd:element ref="INVOICE_PARTY" minOccurs="0"/>
				<xsd:element ref="SHIPMENT_PARTIES" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ORDER_REFERENCE">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ORDER_ID"/>
				<xsd:element ref="LINE_ITEM_ID"/>
				<xsd:element ref="ORDER_DATE" minOccurs="0"/>
				<xsd:element ref="AGREEMENT" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="CATALOG_REFERENCE" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ORDER_UNIT" type="dtPUNIT"/>
	<xsd:element name="PACKAGE_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PACKAGE_INFO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PACKAGE_ID"/>
				<xsd:element ref="PACKAGE_TYPE" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PACKAGE_TYPE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PARTIAL_SHIPMENT" type="dtBOOLEAN"/>
	<xsd:element name="PARTIAL_SHIPMENT_ALLOWED" type="dtBOOLEAN"/>
	<xsd:element name="PARTY">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PARTY_ID" minOccurs="0"/>
				<xsd:element ref="ADDRESS" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PARTY_ID">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typePARTY_ID">
					<xsd:attribute name="type" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:maxLength value="250"/>
								<xsd:minLength value="1"/>
								<!-- ***** predefined values for attribute 'type' of element 'PARTY_ID' *****
									"(duns | iln | buyer_specific | supplier_specific)">
								-->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PAYMENT">
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="CARD"/>
				<xsd:element ref="ACCOUNT"/>
				<xsd:element ref="DEBIT"/>
				<xsd:element ref="CHECK"/>
				<xsd:element ref="CASH"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PAYMENT_TERM">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typePAYMENT_TERM">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:NMTOKEN">
								<xsd:enumeration value="unece"/>
								<xsd:enumeration value="other"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PHONE">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typePHONE">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:NMTOKEN">
								<xsd:enumeration value="office"/>
								<xsd:enumeration value="mobile"/>
								<xsd:enumeration value="private"/>
								<xsd:enumeration value="other"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PRICE_AMOUNT" type="dtNUMBER"/>
	<xsd:element name="PRICE_CURRENCY" type="dtCURRENCIES"/>
	<xsd:element name="PRICE_FLAG">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="dtBOOLEAN">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="50"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PRICE_LINE_AMOUNT" type="dtNUMBER"/>
	<xsd:element name="PRICE_QUANTITY" type="dtNUMBER"/>
	<xsd:element name="PUBLIC_KEY">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typePUBLIC_KEY">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:maxLength value="50"/>
								<xsd:minLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="QUANTITY" type="dtNUMBER"/>
	<xsd:element name="QUOTATION_DATE" type="dtDATETIME"/>
	<xsd:element name="QUOTATION_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="RECEIPTACKNOWLEDGEMENT_DATE" type="dtDATETIME"/>
	<xsd:element name="RECEIPTACKNOWLEDGEMENT_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="RECEIPT_DATE" type="dtDATETIME"/>
	<xsd:element name="REMARK">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typeREMARK">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="250"/>
								<!-- ***** predefined values for attribute 'type' of element 'REMARK' *****
									"(general | rfq | quotation | order | orderchange | orderresponse | dispatchnotifiaction | 
									receiveacknoledgement | invoice | deliverynote | transport)"
								-->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="RFQ_DATE" type="dtDATETIME"/>
	<xsd:element name="RFQ_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SHIPMENT_PARTIES">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DELIVERY_PARTY"/>
				<xsd:element ref="FINAL_DELIVERY_PARTY" minOccurs="0"/>
				<xsd:element ref="TRANSPORT_PARTY" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SOURCING_INFO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="QUOTATION_ID" minOccurs="0"/>
				<xsd:element ref="AGREEMENT" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="CATALOG_REFERENCE" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
		<!-- ***** START deactivate identity-constraints ***** -->
		<xsd:key name="keyAGREEMENT">
			<xsd:selector xpath="AGREEMENT"/>
			<xsd:field xpath="AGREEMENT_ID"/>
			<xsd:field xpath="AGREEMENT_LINE_ID"/>
		</xsd:key>
		<!-- ***** END deactivate identity-constraints   ***** -->
	</xsd:element>
	<xsd:element name="SPECIAL_TREATMENT_CLASS">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typeSPECIAL_TREATMENT_CLASS">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:maxLength value="50"/>
								<xsd:minLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="STATE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="STOP_AUTOMATIC_PROCESSING">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="STREET">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SUPPLIER_AID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="32"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SUPPLIER_ORDER_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SUPPLIER_ORDER_ITEM_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SUPPLIER_ORDER_REFERENCE">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SUPPLIER_ORDER_ID"/>
				<xsd:element ref="SUPPLIER_ORDER_ITEM_ID"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SUPPLIER_PARTY">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PARTY"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="TAX" type="dtNUMBER"/>
	<xsd:element name="TAX_AMOUNT" type="dtNUMBER"/>
	<xsd:element name="TERMS_AND_CONDITIONS">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="TOTAL_AMOUNT" type="dtNUMBER"/>
	<xsd:element name="TOTAL_ITEM_NUM" type="dt_COUNT"/>
	<xsd:element name="TOTAL_TAX_AMOUNT" type="dtNUMBER"/>
	<xsd:element name="TRANSPORT">
