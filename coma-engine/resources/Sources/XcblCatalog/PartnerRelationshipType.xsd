    <xsd:complexType name="PartnerRelationshipType">
        <xsd:annotation>
            <xsd:documentation>identifies the role of the partners.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="PartnerRelationshipCoded" type="PartnerRelationshipCodeType">
                <xsd:annotation>
                    <xsd:documentation>describes the relationship/role of the partner who the catalog is targeted at.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PartnerRelationshipCodedOther" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>is used to provide a non-standard <!--code-->PartnerRelationshipCode<!--/code-->.
        This element is mandatory if the value of <!--code-->PartnerRelationshipCode<!--/code--> is
        'Other'. These codes should not contain white space unless absolutely
        necessary.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
