    <xsd:complexType name="ActionType">
        <xsd:annotation>
            <xsd:documentation>specifies the action to be taken by the application processing the product description.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="Value" type="core:ActionValueCodeType" use="optional" default="Add">
            <xsd:annotation>
                <xsd:documentation>
            indicates the action to be taken: <!--code-->Add<!--/code-->,
        <!--code-->Update<!--/code-->, <!--code-->Replace<!--/code-->, or <!--code-->Delete<!--/code-->. When no <!--code-->Action<!--/code--> is
        present a default is assumed of <!--code-->Add<!--/code-->in the
        <!--code-->CatalogHeader<!--/code-->.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
