    <xsd:complexType name="CatalogSystemType">
        <xsd:annotation>
            <xsd:documentation>specifies the system address and type of system of this
          catalog. Contains 2 mandatory subelements.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="SystemAddress" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>supplies a system address supplied by the provider's catalog
          system.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SystemType" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>gives a description of the type of system used by the provider to
          create the catalog.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
