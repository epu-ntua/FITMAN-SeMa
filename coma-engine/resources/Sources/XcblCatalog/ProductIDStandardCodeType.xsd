    <xsd:simpleType name="ProductIDStandardCodeType">
        <xsd:annotation>
            <xsd:documentation>This code identifies the standard that the product ID is produced to.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:NMTOKEN">
            <xsd:enumeration value="Other">
                <xsd:annotation>
                    <xsd:documentation>Custom Code</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="GTIN">
                <xsd:annotation>
                    <xsd:documentation>Global Trade Item Number</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="EAN">
                <xsd:annotation>
                    <xsd:documentation>International Article Numbering Association</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UCC">
                <xsd:annotation>
                    <xsd:documentation>Uniform Code Council</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>
