    <xsd:complexType name="VisibilityRulesType">
        <xsd:annotation>
            <xsd:documentation>indicates the visibility level of the auction. If the information
        is not provided, then the assumption is made that the auction is public and
        that all values are visible. </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="VisibilityIndicator" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>is a general indicator to specify whether or not the auction is
        public or private.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="VisibilityOfComments" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates if an auction participant can access the comments of
        other participants.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="VisibilityOfAmounts" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates if an auction participant can access the bid amounts of
        other participants.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="VisibilityOfQuantities" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates if an auction participant can access the bid quantities
        of other participants.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="VisibilityOfParticipants" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates if an auction participant can access the identities of
        other participants.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
