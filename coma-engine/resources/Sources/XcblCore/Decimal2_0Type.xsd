    <xsd:simpleType name="Decimal2_0Type">
        <xsd:annotation>
            <xsd:documentation>is intended to hold a decimal double with up
    to 2 digits of precision, including up to 2 digits to the left of the decimal
    sign and 0 digits to the right.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal">
            <xsd:totalDigits value="2"/>
            <xsd:fractionDigits value="0"/>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>
