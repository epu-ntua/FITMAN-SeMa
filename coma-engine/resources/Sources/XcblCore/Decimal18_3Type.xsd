    <xsd:simpleType name="Decimal18_3Type">
        <xsd:annotation>
            <xsd:documentation>is intended to hold a
    decimal double with up to 18 digits of precision,
    including up to 15 digits to the left of the decimal sign
    and 3 digits to the right.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal">
            <xsd:totalDigits value="18"/>
            <xsd:fractionDigits value="3"/>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>
