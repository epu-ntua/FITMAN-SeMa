    <xsd:simpleType name="AcademicTitleCodeType">
        <xsd:annotation>
            <xsd:documentation>This code identifies the academic title of the user.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:NMTOKEN">
            <xsd:enumeration value="Other">
                <xsd:annotation>
                    <xsd:documentation>Custom Code</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Prof">
                <xsd:annotation>
                    <xsd:documentation>Professor</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Dr">
                <xsd:annotation>
                    <xsd:documentation>Doctor</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Magister">
                <xsd:annotation>
                    <xsd:documentation>Master</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Phd">
                <xsd:annotation>
                    <xsd:documentation>Doctorate</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="MBA">
                <xsd:annotation>
                    <xsd:documentation>Master of Business Administration</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BA">
                <xsd:annotation>
                    <xsd:documentation>Bachellor of Arts</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
