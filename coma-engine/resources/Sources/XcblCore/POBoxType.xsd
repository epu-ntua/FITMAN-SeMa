    <xsd:complexType name="POBoxType">
        <xsd:annotation>
            <xsd:documentation>contains the post office box number.  This is a string content model.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute name="POBoxPostalCode" type="xsd:string" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>
            is used to hold the postal code associated with the post office box.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
</xsd:schema>
