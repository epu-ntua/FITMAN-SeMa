                    <xsd:documentation>Custom Code</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10DaysAfterEndOfMonth">
                <xsd:annotation>
                    <xsd:documentation>Payment due ten days after end of a calendar month.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="AdjustmentPayment">
                <xsd:annotation>
                    <xsd:documentation>Payment adjusted for outstanding credits or debits.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="AdvancedPayment">
                <xsd:annotation>
                    <xsd:documentation>Payment in advance of due date.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
