                    <xsd:documentation>Payment made by negotiation with any bank in a specified location.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="AvailableBySightPayment">
                <xsd:annotation>
                    <xsd:documentation>Payment made at sight.</xsd:documentation>
                </xsd:annotation>
