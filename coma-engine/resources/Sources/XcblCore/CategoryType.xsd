    <xsd:complexType name="CategoryType">
        <xsd:annotation>
            <xsd:documentation>contains the product category.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CategoryID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>is the material group number.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="StandardCategoryID" type="StandardCategoryIDType">
                <xsd:annotation>
                    <xsd:documentation>is the standard material group number in accordance with UNSPSC or
        eClass.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
