            at http://www.xcbl.org/license.html
        </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType name="GPSCoordinatesType">
        <xsd:annotation>
            <xsd:documentation>identifies the location by specific co-ordinates.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="GPSSystem" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>identifies the GPS system.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Latitude" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>specifies the latitude of the location.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Longitude" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>specifies the longitude of the location.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
