    <xsd:simpleType name="DateTimeRefCodeType">
        <xsd:annotation>
            <xsd:documentation>This code references to a point in time. This code list is derived from EDIFACT 2475 (Payment time reference code) and X12 333 (Term basis date code)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:NMTOKEN">
            <xsd:enumeration value="Other">
                <xsd:annotation>
                    <xsd:documentation>Custom Code</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="AnticipatedDeliveryDate">
                <xsd:annotation>
                    <xsd:documentation>The date on which delivery is anticipated to take place.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ApprovalOfGageDate">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ApprovalOfSampleDate">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="CompletionAndAcceptanceDate">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="CustomsClearanceDateExport">
                <xsd:annotation>
                    <xsd:documentation>Date when goods clear Customs in the exporting country.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="CustomsClearanceDateImport">
                <xsd:annotation>
                    <xsd:documentation>Date when goods clear Customs in the importing country.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateCreditNoteReceived">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of credit note received.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateExWorks">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date when goods are leaving the sellers factory.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateGoodsHandedOverForShipmentBySellerOrAgent">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateGoodsReceivedByBuyer">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date when goods are received by buyer.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateGoodsReceivedByBuyersAgent">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date when goods are received by buyer's agent.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateGoodsReceivedByCarrier">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date when goods are received by carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateInvoiceReceived">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of invoice received.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfAcceptanceOfFirstSamplesProducedUnderProductionConditions">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of acceptance of first samples produced under production conditions.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfAcceptanceOfNegotiableInstrument">
                <xsd:annotation>
                    <xsd:documentation>Date when the negotiable instrument is accepted or will be accepted by drawee.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfAcceptanceOfTooling">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of acceptance of tooling or set of tooling.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfArrivalOfTransport">
                <xsd:annotation>
                    <xsd:documentation>Date the transport arrived at the agreed destination.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfBillOfLadingConsignmentNoteOrOtherTransportDocument">
                <xsd:annotation>
                    <xsd:documentation>The date of issuance of a bill of lading, consignment note or similar transport document.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfCertificateOfFinalAcceptance">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfCertificateOfPreliminaryAcceptance">
                <xsd:annotation>
                    <xsd:documentation>Date of certificate of temporary acceptance of work until final reception will take place.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfConfirmation">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of confirmation.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfConfirmationOfOrderReceived">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of confirmation received.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfContract">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of contract.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfCreditNote">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of credit note.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DeliveryDate">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfDeliveryOfGoodsToEstablishmentsDomicileSite">
                <xsd:annotation>
                    <xsd:documentation>Date the goods are delivered at agreed place of destination.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfDeliveryToBuyerOfDocumentsRepresentingGoods">
                <xsd:annotation>
                    <xsd:documentation>Date when documents representing goods are received by buyer .</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfDeliveryToBuyersAgentOfDocumentsRepresentingGoods">
                <xsd:annotation>
                    <xsd:documentation>Date when documents representing goods are received by buyer's agent.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfDeliveryToCarrierOfDocumentsRepresentingGoods">
                <xsd:annotation>
                    <xsd:documentation>Date when documents representing goods are received by carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfDeliveryToIntermediaryBankOfDocumentsRepresentingGood">
                <xsd:annotation>
                    <xsd:documentation>Date when documents representing goods are received by intermediary bank.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfEndOfWork">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfFinalAcceptanceOfWork">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfInvoice">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of invoice.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfInwardFrontierCrossing">
                <xsd:annotation>
                    <xsd:documentation>Date the goods are crossing the border of the importers country.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfIssueOfTransportDocument">
                <xsd:annotation>
                    <xsd:documentation>The date on which a transport document(s) is issued.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfNegotiableInstrumentDraftPromissoryNoteBank">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfOrder">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of order.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfOutwardFrontierCrossing">
                <xsd:annotation>
                    <xsd:documentation>Date the goods are crossing the border of the exporters country.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfPresentationOfDocuments">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfPresentationOfNegotiableInstrument">
                <xsd:annotation>
                    <xsd:documentation>Date when the negotiable instrument is presented or will be presented to will be presented to drawee.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfPresentDocument">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of present document.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfProvisionalReceptionOfWork">
                <xsd:annotation>
                    <xsd:documentation>Date of temporary acceptance of work until final reception will take place.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfReceiptForLoadingMatesReceipt">
                <xsd:annotation>
                    <xsd:documentation>Date when goods are expedited to a ship confirmed by mate's received.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfReceiptOfToolDependentInitialSamplesPlusUnlimitedAbsoluteBankGuaranteePlusValueAddedTax">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of receipt of tool dependent initial samples and unlimited absolute bank guarantee of a third party, who is liable to the full amount of the tooling (which is owned by the customer).</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfReceiptOfTooling">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of receipt of tooling or set of tooling.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfResaleByBuyer">
                <xsd:annotation>
                    <xsd:documentation>Payment time reference is date of resale by the buyer.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfSalaryPayment">
                <xsd:annotation>
                    <xsd:documentation>Date when a salary payment was made.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ShipDate">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DateOfShipmentAsEvidencedByTheTransportDocument">
                <xsd:annotation>
                    <xsd:documentation>Date of shipment as evidenced by the transport document(s).</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
