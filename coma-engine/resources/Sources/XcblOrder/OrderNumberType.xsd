    <xsd:complexType name="OrderNumberType">
        <xsd:annotation>
            <xsd:documentation>is a unique number assigned to the <!--code-->Order<!--/code--> in respect
        to the parties assigning the number.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="BuyerOrderNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>is the unique number assigned by the buyer to the
        <!--code-->Order<!--/code-->. </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SellerOrderNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>is the unique number assigned by the seller to the
        <!--code-->Order<!--/code-->.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
